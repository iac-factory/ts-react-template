{
    "name": "template",
    "version": "0.0.0",
    "lockFileVersion": 3,
    "private": true,
    "type": "module",
    "contributors": [
        "Ethan Seligman",
        "Jacob B. Sanders"
    ],
    "devDependencies": {
        "lerna": "^4.0.0"
    },
    "repository": {
        "type": "git",
        "directory": "/",
        "url": "git+https://gitlab.com/pastel-cloud/react/template.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "scripts": {
        "install": "lerna bootstrap --npm-client npm",
        "postinstall": "lerna link",
        "start": "lerna run start --stream",
        "release": "lerna version",
        "postversion": "lerna publish from-git --canary",
        "ci": "npm run release"
    }
}
